#summary
  ul.summary-list
    li
      div
        span 分数
        i(class='fa fa-star-o fa-lg pull-right', aria-hidden='true')
      .summary-content
        span#score 
        span /100
    li
      div
        span 覆盖率
        i(class='fa fa-check-square-o fa-lg pull-right', aria-hidden="true")
      .summary-content
        div
          span 代码覆盖率：
          span#testCover.emphasize-num
          span %
        div
          span 包覆盖率：
          span#testPkgCover.emphasize-num
          span %
    li
      div
        span Issues
        i(class='fa fa-info-circle fa-lg pull-right', aria-hidden="true")
      .summary-content
        div
          span 问题总数：
          span#goIssueNum.emphasize-num
    li
      div
        span 圈复杂度
        i(class='fa fa-life-ring fa-lg pull-right', aria-hidden="true")
      .summary-content
        div
          div
            span 15~50分文件数：
            span#mediumCycleNum.emphasize-num
          div
            span 50分以上文件数：
            span#highCycleNum.emphasize-num
    li
      div
        span 代码量
        i(class='fa fa-life-ring fa-lg pull-right', aria-hidden="true")
      .summary-content
        div
        span#codeLineNum.emphasize-num

  .content
    .row.chart-container
      .col-sm-7.col-md-7
        span.chart-title
          span.chart-legend-block.green
          span.chart-legend-block.purple
          .
            单元测试
        #gotestChart
      .col-sm-5.col-md-5
        .pie-chart
          div
            span.chart-title
              span.chart-legend-block.grey
              .
                包圈复杂度
            #gocycleChart
          div
            span.chart-title
              span.chart-legend-block.orange
              span 包Issue
            #goIssue
          div
            span.chart-title
              span.chart-legend-block.lavande
              .
                包代码量占比
            #goPercentage
    .chart-container




